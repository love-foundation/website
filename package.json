{
  "name": "love_foundation_website",
  "description": "Love Foundation Website",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "build:staging": "cross-env ADAPTER=node svelte-kit build",
    "cy:run": "cypress run --headless",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "test-dev": "run-p --race dev cy:open"
  },
  "engine": {
    "node": "16.13.2",
    "npm": "8.4.0"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 safari versions"
  ],
  "dependencies": {
    "@directus/sdk": "^9.8.0",
    "cross-env": "^7.0.3",
    "lozad": "^1.16.0"
  },
  "devDependencies": {
    "@beyonk/gdpr-cookie-consent-banner": "^9.0.3",
    "@sveltejs/adapter-node": "^1.0.0-next.67",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.4.2",
    "bulma": "^0.9.3",
    "cypress": "^8.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "fuse.js": "^6.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "sass": "^1.55.0",
    "svelte": "^3.51.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-select": "^3.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
